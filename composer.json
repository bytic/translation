{
  "name": "bytic/translation",
  "description": "Bytic translation component",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\I18n\\": "src/"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/functions/common.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\I18n\\Tests\\": "tests/",
      "Nip\\I18n\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.1|^8.0",
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/utility": "^1.0",
    "bytic/collections": "^1.0",
    "bytic/filesystem": "^0.9|^1.0",
    "bytic/locale-detector": "^1.0",
    "symfony/finder": "^4.0|^5.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "bytic/application": "^0.9|^1.0",
    "bytic/router": "^0.9|^1.0",
    "phpunit/phpunit": "^7.2|^8.0|^9.0",
    "mockery/mockery": "^1.0"
  }
}
