{
  "name": "bytic/translation",
  "description": "Bytic translation component",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\I18n\\": "src/"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/functions/common.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\I18n\\Tests\\": "tests/src",
      "Nip\\I18n\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "bytic/package-base": "^1.0",
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/utility": "^1.0",
    "bytic/collections": "^1.0",
    "bytic/filesystem": "^0.9|^1.0",
    "bytic/locale-detector": "^1.0",
    "symfony/finder": "^4.0|^5.0|^6.0",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "bytic/application": "^0.9|^1.0",
    "bytic/router": "^0.9|^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
